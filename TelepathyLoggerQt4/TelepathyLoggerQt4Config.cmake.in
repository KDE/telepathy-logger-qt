# TelepathyLoggerQt4Config.cmake is generated by CMake from TelepathyQt/TelepathyQtConfig.cmake.in.
# Any changed value in this file will be overwritten by CMake.

if(NOT TelepathyLoggerQt4_FOUND)
    # set the version number
    set(TELEPATHY_LOGGER_QT4_VERSION_MAJOR @TP_LOGGER_QT4_MAJOR_VERSION@)
    set(TELEPATHY_LOGGER_QT4_VERSION_MINOR @TP_LOGGER_QT4_MINOR_VERSION@)
    set(TELEPATHY_LOGGER_QT4_VERSION_MICRO @TP_LOGGER_QT4_MICRO_VERSION@)
    set(TELEPATHY_LOGGER_QT4_VERSION_NANO @TP_LOGGER_QT4_NANO_VERSION@)
    set(TELEPATHY_LOGGER_QT4_VERSION @PACKAGE_VERSION@)

    # set the directories
    if(NOT TELEPATHY_LOGGER_QT4_INSTALL_DIR)
    set(TELEPATHY_LOGGER_QT4_INSTALL_DIR "@CMAKE_INSTALL_PREFIX@")
    endif(NOT TELEPATHY_LOGGER_QT4_INSTALL_DIR)

    set(TELEPATHY_LOGGER_QT4_INCLUDE_DIR   "@TELEPATHY_LOGGER_QT4_INCLUDE_DIR@")
    set(TELEPATHY_LOGGER_QT4_DEFINITIONS   "@TELEPATHY_LOGGER_QT4_DEFINITIONS@")
    set(TELEPATHY_LOGGER_QT4_INCLUDE_DIRS  ${TELEPATHY_LOGGER_QT4_INCLUDE_DIR}
                                           @GLIB2_INCLUDE_DIR@
                                           @QTGLIB_INCLUDE_DIR@)

    # For backwards compatibility
    set(TELEPATHY_LOGGER_QT4_FOUND TRUE)

    # attempt to find the generated TelepathyLoggerQt4Targets.cmake in the same directory
    get_filename_component(_TP_LOGGER_QT4_CONFIG_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
    find_file(_TP_LOGGER_QT4_TARGETS_FILE TelepathyLoggerQt4Targets.cmake PATHS ${_TP_LOGGER_QT4_CONFIG_DIR} NO_DEFAULT_PATH)

    # set the TELEPATHY_LOGGER_QT4_LIBRARIES variable
    include(${_TP_LOGGER_QT4_TARGETS_FILE})
    set(TELEPATHY_LOGGER_QT4_LIBRARIES telepathy-logger-qt4)
endif(NOT TelepathyLoggerQt4_FOUND)

